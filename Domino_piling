import java.util.*;
public class Domino{
    public static void main(String args[]){
        Scanner s=new Scanner( System.in);
      //  System.out.println("enter the M:");
        int m=s.nextInt();
       // System.out.println("enter the N:");
        int n=s.nextInt();
        int res=(m*n)/2;
        System.out.println(res);
    }
}
